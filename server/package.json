{
  "name": "agentpay-server",
  "version": "1.1.0",
  "description": "AgentPay server integrating Hedera, LangChain, and Google GenAI",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "start:prod": "npm run build && node dist/src/index.js",
    "create-token": "node --loader ts-node/esm scripts/createToken.ts",
    "associate-token": "node --loader ts-node/esm scripts/associateToken.ts",
    "test-transfer": "node --loader ts-node/esm scripts/testTransfer.ts",
    "check-balances": "node --loader ts-node/esm scripts/checkBalances.ts",
    "test-agent": "node --loader ts-node/esm scripts/testAgent.ts"
  },
  "keywords": [
    "hedera",
    "agent",
    "langchain",
    "google-genai",
    "express",
    "typescript"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@hashgraph/sdk": "^2.37.0",
    "@langchain/google-genai": "^0.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "langchain": "^0.0.203"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
