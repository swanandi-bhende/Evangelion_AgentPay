{
  "name": "agentpay-server",
  "version": "1.0.0",
  "description": "AgentPay server integrating Hedera, LangChain, and Google GenAI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "create-token": "ts-node scripts/createToken.ts",
    "associate-token": "ts-node scripts/associateToken.ts",
    "test-transfer": "ts-node scripts/testTransfer.ts",
    "check-balances": "ts-node scripts/checkBalances.ts",
    "test-agent": "node --loader ts-node/esm scripts/testAgent.ts"
  },
  "keywords": [
    "hedera",
    "agent",
    "langchain",
    "google-genai",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.17.0",
    "@hashgraph/sdk": "^2.75.0",
    "@langchain/google-genai": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "hedera-agent-kit": "^3.4.0",
    "helmet": "^8.1.0",
    "langchain": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
