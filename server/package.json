{
  "name": "agentpay-server",
  "version": "1.1.0",
  "description": "AgentPay server integrating Hedera, LangChain, and Google GenAI",
  "main": "dist/src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "SET NODE_ENV=development& tsx watch src/index.ts",
    "dev:unix": "NODE_ENV=development tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "start:prod": "SET NODE_ENV=production& npm run build && npm run start",
    "start:prod:unix": "NODE_ENV=production npm run build && npm run start",
    "create-token": "tsx scripts/createToken.ts",
    "associate-token": "tsx scripts/associateToken.ts",
    "test-transfer": "tsx scripts/testTransfer.ts",
    "check-balances": "tsx scripts/checkBalances.ts",
    "test-agent": "tsx scripts/testAgent.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "hedera",
    "agent",
    "langchain",
    "google-genai",
    "express",
    "typescript"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@hashgraph/sdk": "^2.37.0",
    "@langchain/core": "^0.1.48",
    "@langchain/google-genai": "^0.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "langchain": "^0.1.25",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
}
